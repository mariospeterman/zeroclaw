{
  "name": "right-hand-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:tauri": "node ./scripts/dev-tauri.mjs",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "node ./scripts/tauri-cli.mjs",
    "android:dev": "npm run tauri -- android dev",
    "android:studio": "npm run tauri -- android dev --open",
