    "android:build:debug": "npm run tauri -- android build -- --debug --apk -t x86_64 -t aarch64",
    "android:reset-lock": "bash ./scripts/android-reset-lock.sh"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.8.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.8.0",
    "cross-env": "^7.0.3",
    "vite": "^5.4.0"
  }
}
